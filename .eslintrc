{
  "extends": ["airbnb", "plugin:prettier/recommended", "plugin:react/recommended", "plugin:jsx-a11y/recommended"],
  "plugins": ["prettier", "react"],
  "rules": {
    "prettier/prettier": ["error"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
